// prepositions
a            _%a          // at
al           _%al
de           $u           // of,from
del          $u
(de la)      d,ela
en           _%En
con          $u $pause    // with
por          $u $pause    // for,through,along,via
para         $u $pause    // for,in order,by
entre        $u $pause
desde        $pause       // from,since
después      $pause       // after

// conjunctions
y            %i $pause    // and
o            $u $pause    // or

así          $pause
aunque       $u $pause    // although
siquiera     $pause


// questions
que          $u $pause $strend   // whar
cuál         $pause       // which
cuáles       $pause
cómo         $pause       // how


